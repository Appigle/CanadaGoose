{
  "name": "server",
  "version": "3.0.2",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "NODE_ENV=production PORT=3000 node app.js",
    "dev": "NODE_ENV=development PORT=3000 nodemon app.js",
    "prod": "NODE_ENV=production PORT=3000 node app.js",
    "test": "NODE_ENV=test mocha test/*.js --timeout 10000 --exit",
    "test:coverage": "NODE_ENV=test nyc mocha test/*.js --timeout 10000 --exit",
    "version:check": "node scripts/check-version.js",
    "version:patch": "node scripts/interactive-version.js --patch --auto",
    "version:minor": "node scripts/interactive-version.js --minor --auto",
    "version:major": "node scripts/interactive-version.js --major --auto",
    "version:interactive": "node scripts/interactive-version.js"
  },
  "keywords": [
    "express",
    "authentication",
    "security",
    "mysql"
  ],
  "author": "",
  "license": "ISC",
  "description": "Express.js backend with JWT authentication and security features",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.11.4"
  },
  "devDependencies": {
    "chai": "^4.5.0",
    "mocha": "^10.8.2",
    "nodemon": "^3.1.9",
    "nyc": "^15.1.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "supertest": "^7.0.0"
  }
}
